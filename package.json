    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rollup": "^2.44.0",
    "rollup-plugin-dts": "^3.0.1",
    "typedoc": "^0.22.6",
